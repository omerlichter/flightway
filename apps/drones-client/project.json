{
  "name": "drones-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/drones-client/src",
  "tags": ["client"],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/drones-client",
        "index": "apps/drones-client/src/index.html",
        "main": "apps/drones-client/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/drones-client/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/drones-client/src/favicon.ico",
          "apps/drones-client/src/assets",
          {
            "glob": "**/*",
            "input": "./node_modules/leaflet/dist/images/",
            "output": "./"
          }
        ],
        "styles": [
          "apps/drones-client/src/styles/styles.scss",
          "node_modules/leaflet/dist/leaflet.css",
          {
            "input": "apps/drones-client/src/styles/themes/drones/light/theme.scss",
            "bundleName": "drones-light",
            "inject": false
          },
          {
            "input": "apps/drones-client/src/styles/themes/drones/dark/theme.scss",
            "bundleName": "drones-dark",
            "inject": false
          }
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "drones-client:build:production"
        },
        "development": {
          "buildTarget": "drones-client:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/drones-client/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "drones-client:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/drones-client/jest.config.ts"
      }
    }
  }
}
